export class BlogItem {
  public name: String;
  public content: String;
  public imagePath: String;
  public genre: String;
  public btype: String;
  public bnumber: String;
  public isActive: boolean;


  constructor(name: String, content: String, imagePath: String, genre: String, btype: String, bnumber: String ) {
    this.name = name;
    this.content = content;
    this.imagePath = imagePath;
  }
}
